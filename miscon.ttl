                              rdfs:label "HierarchicallyEquivalentWith"@en .


###  https://purl.org/s2c2/miscon#isCommanderOf
:isCommanderOf rdf:type owl:ObjectProperty ;
               owl:inverseOf :isSubordinateTo ;
               rdfs:comment "A relationship that establishes that a military organization is commander of another military organization," ;
               rdfs:label "isCommanderOf"@pt .


###  https://purl.org/s2c2/miscon#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             rdfs:domain :MilitaryPersonAsPassenger ;
             rdfs:range :Vehicle ;
             rdfs:comment "A relationship that establishes that a passenger is located in a vehicle." ;
             rdfs:label "isLocatedIn"@en .


###  https://purl.org/s2c2/miscon#isSubordinateTo
:isSubordinateTo rdf:type owl:ObjectProperty ;
                 rdfs:domain :Subordinate ;
                 rdfs:range :Commander ;
                 rdfs:comment "A relationship that establishes that a military organization is subordinate to another military organization." ;
                 rdfs:label "isSubordinateTo"@en .


###  https://purl.org/s2c2/miscon#mayTalkTo
:mayTalkTo rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdf:type owl:SymmetricProperty ;
           rdfs:domain :Ue ;
           rdfs:range :Ue ;
           rdfs:comment "A relationship that establishes that an interface may talk to another interface." ;
           rdfs:label "mayTalkTo" .


###  https://purl.org/s2c2/miscon#militaryPersonHasMilitaryOrganization
:militaryPersonHasMilitaryOrganization rdf:type owl:ObjectProperty ;
                                       rdfs:domain :MilitaryPerson ;
                                       rdfs:range :MilitaryOrganization .


###  https://purl.org/s2c2/miscon#militaryPersonIsSubordinateTo
:militaryPersonIsSubordinateTo rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf owl:topObjectProperty ;
                               rdfs:domain :SubordinateMilitaryPerson ;
                               rdfs:range :CommanderMilitaryPerson ;
                               rdfs:comment "A relationship that defines the relationship of subordination between two military persons from the same MO" ;
                               rdfs:label "militaryPersonIsSubordinateTo" .


###  https://purl.org/s2c2/miscon#militaryScenarioHasInstitutionalElement
:militaryScenarioHasInstitutionalElement rdf:type owl:ObjectProperty ;
                                         rdfs:domain :MilitaryScenario ;
                                         rdfs:range :InstitutionalElement ;
                                         rdfs:comment "A relationship that establishes that a military scenario has an institutional element." ;
                                         rdfs:label "militaryScenarioHasInstitutionalElement" .


###  https://purl.org/s2c2/miscon#militaryScenarioHasOperationalElement
:militaryScenarioHasOperationalElement rdf:type owl:ObjectProperty ;
                                       rdfs:domain :MilitaryScenario ;
                                       rdfs:range :OperationalElement ;
                                       rdfs:comment "A relationship that establishes that a military scenario has an operational element." ;
                                       rdfs:label "militaryScenarioHasOperationalElement" .


###  https://purl.org/s2c2/miscon#networkHasInterface
:networkHasInterface rdf:type owl:ObjectProperty ;
                     rdfs:domain :WirelessNetwork ;
                     rdfs:range :Interface ;
                     rdfs:comment "A relationship that establishes that a WirelessNetwork has an interface." ;
                     rdfs:label "networkHasInterface" .


###  https://purl.org/s2c2/miscon#operates
:operates rdf:type owl:ObjectProperty ;
          rdfs:domain :UeOperator ;
          rdfs:range :Ue ;
          rdfs:comment "A relationship that establishes that a person operates an User equipment." ;
          rdfs:label "operates"@en .


