gufo1:Role rdf:type owl:Class .


###  http://purl.org/nemo/gufo#SubKind
gufo1:SubKind rdf:type owl:Class .


###  https://purl.org/s2c2/miscon#AccessPoint
:AccessPoint rdf:type owl:Class ;
             rdfs:subClassOf :Interface .


###  https://purl.org/s2c2/miscon#Armored
:Armored rdf:type owl:Class ;
         rdfs:subClassOf :Vehicle .


###  https://purl.org/s2c2/miscon#Battalion
:Battalion rdf:type owl:Class ;
           rdfs:subClassOf :MilitaryOrganization .


###  https://purl.org/s2c2/miscon#BattleGroup
:BattleGroup rdf:type owl:Class ;
             rdfs:subClassOf :MilitaryOrganization .


###  https://purl.org/s2c2/miscon#Brigade
:Brigade rdf:type owl:Class ;
         rdfs:subClassOf :MilitaryOrganization .


###  https://purl.org/s2c2/miscon#Cascavel
:Cascavel rdf:type owl:Class ;
          rdfs:subClassOf :Armored .


###  https://purl.org/s2c2/miscon#CommDevice
:CommDevice rdf:type owl:Class ;
            rdfs:subClassOf :OperationalElement .


###  https://purl.org/s2c2/miscon#CommDeviceCarrier
:CommDeviceCarrier rdf:type owl:Class ;
                   rdfs:subClassOf gufo1:FunctionalComplex .


###  https://purl.org/s2c2/miscon#Commander
:Commander rdf:type owl:Class ;
           rdfs:subClassOf :MilitaryOrganization .


###  https://purl.org/s2c2/miscon#CommanderMilitaryPerson
:CommanderMilitaryPerson rdf:type owl:Class ;
                         rdfs:subClassOf :MilitaryPerson ;
                         owl:disjointWith :SubordinateMilitaryPerson .


###  https://purl.org/s2c2/miscon#Company
:Company rdf:type owl:Class ;
         rdfs:subClassOf :MilitaryOrganization .


###  https://purl.org/s2c2/miscon#Guarani
:Guarani rdf:type owl:Class ;
         rdfs:subClassOf :Armored .


###  https://purl.org/s2c2/miscon#HierarchicallyEquivalent
:HierarchicallyEquivalent rdf:type owl:Class ;
                          rdfs:subClassOf :MilitaryOrganization ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :hierarchicallyEquivalentWith ;
                                            owl:someValuesFrom :HierarchicallyEquivalent
                                          ] .


###  https://purl.org/s2c2/miscon#InstitutionalElement
:InstitutionalElement rdf:type owl:Class ;
                      rdfs:subClassOf gufo1:FunctionalComplex ;
                      rdfs:comment "Institutional Elements of the scenario." ;
                      rdfs:label "InstitutionalElement" .


###  https://purl.org/s2c2/miscon#Interface
:Interface rdf:type owl:Class ;
           rdfs:subClassOf gufo1:FunctionalComplex .


###  https://purl.org/s2c2/miscon#MilitaryEmployment
:MilitaryEmployment rdf:type owl:Class ;
                    rdfs:subClassOf gufo1:Relator ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty gufo1:mediates ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :MilitaryOrganization
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty gufo1:mediates ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :MilitaryPerson
                                    ] ;
                    rdfs:comment "The Thruthmaker of the relationship between Military and Military Organization." ;
                    rdfs:label "MilitaryEmployment" .


###  https://purl.org/s2c2/miscon#MilitaryOrganization
:MilitaryOrganization rdf:type owl:Class ;
                      rdfs:subClassOf :InstitutionalElement ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf gufo1:mediates
                                                       ] ;
                                        owl:someValuesFrom :MilitaryEmployment
                                      ] .


###  https://purl.org/s2c2/miscon#MilitaryOrganizationPowerType
:MilitaryOrganizationPowerType rdf:type owl:Class ;
                               rdfs:subClassOf gufo1:EndurantType ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :MOTypeIsSubordinateTo ;
                                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onClass :MilitaryOrganizationPowerType
                                               ] ;
                               rdfs:comment "PowerType of MilitaryOrganization." ;
                               rdfs:label "MilitaryOrganitionPowerType" .


###  https://purl.org/s2c2/miscon#MilitaryPerson
:MilitaryPerson rdf:type owl:Class ;
                rdfs:subClassOf :OperationalElement ,
                                :Person ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :militaryPersonHasMilitaryOrganization ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :MilitaryOrganization
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty [ owl:inverseOf gufo1:mediates
                                                 ] ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :MilitaryEmployment
                                ] .


###  https://purl.org/s2c2/miscon#MilitaryPersonAsDismounted
:MilitaryPersonAsDismounted rdf:type owl:Class ;
                            rdfs:subClassOf :MilitaryPerson ;
                            owl:disjointWith :MilitaryPersonAsPassenger .


###  https://purl.org/s2c2/miscon#MilitaryPersonAsPassenger
:MilitaryPersonAsPassenger rdf:type owl:Class ;
                           rdfs:subClassOf :MilitaryPerson ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :isLocatedIn ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass :Vehicle
                                           ] .


###  https://purl.org/s2c2/miscon#MilitaryPlatform
:MilitaryPlatform rdf:type owl:Class ;
                  rdfs:subClassOf :CommDeviceCarrier ,
                                  :OperationalElement .


###  https://purl.org/s2c2/miscon#MilitaryScenario
:MilitaryScenario rdf:type owl:Class ;
                  rdfs:subClassOf gufo1:FunctionalComplex ;
                  rdfs:comment "Military operation scenario. It is related to all operational and institutional elements that aggregate the scenario where the simulation of the military operation will take place." ;
                  rdfs:label "MilitaryScenario" .


###  https://purl.org/s2c2/miscon#OperationalElement
:OperationalElement rdf:type owl:Class ;
                    rdfs:subClassOf gufo1:FunctionalComplex .


###  https://purl.org/s2c2/miscon#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf gufo1:FunctionalComplex .


###  https://purl.org/s2c2/miscon#Platoon
:Platoon rdf:type owl:Class ;
         rdfs:subClassOf :MilitaryOrganization .


###  https://purl.org/s2c2/miscon#Protocol
:Protocol rdf:type owl:Class ;
          rdfs:subClassOf gufo1:IntrinsicMode .


###  https://purl.org/s2c2/miscon#Subordinate
:Subordinate rdf:type owl:Class ;
             rdfs:subClassOf :MilitaryOrganization ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :isSubordinateTo ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Commander
                             ] .


###  https://purl.org/s2c2/miscon#SubordinateMilitaryPerson
:SubordinateMilitaryPerson rdf:type owl:Class ;
                           rdfs:subClassOf :MilitaryPerson .


###  https://purl.org/s2c2/miscon#Ue
:Ue rdf:type owl:Class ;
    rdfs:subClassOf :Interface .


###  https://purl.org/s2c2/miscon#UeDown
:UeDown rdf:type owl:Class ;
        rdfs:subClassOf :Ue .


###  https://purl.org/s2c2/miscon#UeInternDown
:UeInternDown rdf:type owl:Class ;
              rdfs:subClassOf :Ue .


###  https://purl.org/s2c2/miscon#UeOperator
:UeOperator rdf:type owl:Class ;
            rdfs:subClassOf :MilitaryPerson .


###  https://purl.org/s2c2/miscon#UeUp
:UeUp rdf:type owl:Class ;
      rdfs:subClassOf :Ue .


###  https://purl.org/s2c2/miscon#Urutu
:Urutu rdf:type owl:Class ;
