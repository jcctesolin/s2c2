#    Annotation properties
#################################################################

###  http://purl.org/nemo/gufo#categorizes
gufo1:categorizes rdf:type owl:AnnotationProperty .


###  http://purl.org/nemo/gufo#isDerivedFrom
gufo1:isDerivedFrom rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/nemo/gufo#inheresIn
gufo1:inheresIn rdf:type owl:ObjectProperty ;
                rdfs:range :CommDeviceCarrier .


###  http://purl.org/nemo/gufo#mediates
gufo1:mediates rdf:type owl:ObjectProperty .


###  https://purl.org/s2c2/miscon#MOPTIsSubordinateTo
:MOPTIsSubordinateTo rdf:type owl:ObjectProperty .


###  https://purl.org/s2c2/miscon#MOPowerTypeIsSubordinateTo
:MOPowerTypeIsSubordinateTo rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdfs:domain :MilitaryOrganizationPowerType ;
                            rdfs:range :MilitaryOrganizationPowerType ;
                            rdfs:comment "A relationship that establishes that a MOPowerType is subordinate to another MOPowerType." ;
                            rdfs:label "MOPowerTypeIsSubordinateTo" .


###  https://purl.org/s2c2/miscon#MOTypeIsSubordinateTo
:MOTypeIsSubordinateTo rdf:type owl:ObjectProperty .


###  https://purl.org/s2c2/miscon#ProtocolOf
:ProtocolOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf gufo1:inheresIn ;
            rdfs:domain :Protocol ;
